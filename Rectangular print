import java.util.*;
public class R627 {
	public static void main(String[] args){
		
		Scanner s = new Scanner(System.in);
		System.out.println("Please enter the height");
		int height = s.nextInt();
		System.out.println("Please enter the width");
		int width = s.nextInt();
		
		for(int i=1; i<=height*width; i++)
		{
		    System.out.print("*");
		    if(i%width==0)
		    {
		        System.out.println();
		    }
		}
	}
}
